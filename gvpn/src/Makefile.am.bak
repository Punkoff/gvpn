AM_CPPFLAGS = \
	-DPACKAGE_LOCALE_DIR=\""$(prefix)/$(DATADIRNAME)/locale"\" \
	-DPACKAGE_SRC_DIR=\""$(srcdir)"\" \
	-DPACKAGE_DATA_DIR=\""$(datadir)"\" \
	$(GVPN_CFLAGS)

AM_CFLAGS =\
	 -Wall\
	 -g

bin_PROGRAMS = gvpn

gvpn_SOURCES = \
	callbacks.c\
	callbacks.h \
	cfg.c \
	cfg.h \
	comm.c \
	comm.h \
	main.c 

gvpn_LDFLAGS = \
	-Wl,--export-dynamic

gvpn_LDADD = $(GVPN_LIBS)

files_DATA = \
	callbacks.c\
	gvpn.glade \
	icon.png \
	icon_c.png \
	icon_d.png

filesdir = \
	$(pkgdatadir)

other_DATA = 

otherdir = \
	$(pkgdatadir)

EXTRA_DIST = \
	$(other_DATA)\
	$(app_DATA)

SUBDIRS = \
	link

